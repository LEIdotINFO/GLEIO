@prefix : <http://lei.info/voc/> .
@prefix L1: <http://lei.info/voc/l1/> .
@prefix L2: <http://lei.info/voc/l2/> .
@prefix rr: <http://www.gleif.org/data/schema/rr/2016/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://lei.info/voc/> .

<http://lei.info/voc/> rdf:type owl:Ontology ;
                        owl:versionIRI <http://lei.info/voc/> ;
                        rdfs:comment """Contact
Robert Trypuz (robert.trypuz@makolab.com)"""@en ,
                                     """Global Legal Entity Identifier Ontology (GLEIO) for CDF format of LEVEL 1 & 2.

This ontology - Global Legal Entity Identifier Ontology (GLEIO in short) - has been created by MakoLab's R&D Group. GLEIO is a semantic representation of LEI Common Data File format for LEVEL 1 and 2."""@en ;
                        owl:versionInfo "version 1.0"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://lei.info/voc/l1/level1CDFFName
L1:level1CDFFName rdf:type owl:AnnotationProperty ;
                  rdfs:label "level 1 CDF file name"@en .


###  http://lei.info/voc/l1/level1CDFFType
L1:level1CDFFType rdf:type owl:AnnotationProperty ;
                  rdfs:label "level 1 CDF file type name"@en .


###  http://lei.info/voc/l2/level2CDFFName
L2:level2CDFFName rdf:type owl:AnnotationProperty ;
                  rdfs:label "level 2 CDF file name"@en .


###  http://lei.info/voc/l2/level2CDFFType
L2:level2CDFFType rdf:type owl:AnnotationProperty ;
                  rdfs:label "level 2 CDF file type name"@en .


###  http://www.w3.org/2002/07/owl#maxQualifiedCardinality
owl:maxQualifiedCardinality rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#minQualifiedCardinality
owl:minQualifiedCardinality rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#qualifiedCardinality
owl:qualifiedCardinality rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#closeMatch
<http://www.w3.org/2004/02/skos/core#closeMatch> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#definition
<http://www.w3.org/2004/02/skos/core#definition> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#exactMatch
<http://www.w3.org/2004/02/skos/core#exactMatch> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://lei.info/voc/l1/address
L1:address rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           rdfs:domain L1:LegalEntity ;
           rdfs:range L1:Address ;
           L1:level1CDFFType "lei:AddressType"^^xsd:string ;
           rdfs:label "has address"@en ;
           <http://www.w3.org/2004/02/skos/core#definition> "An address of the legal entity"@en .


###  http://lei.info/voc/l1/alternativeLanguageHeadquartersAddress
L1:alternativeLanguageHeadquartersAddress rdf:type owl:ObjectProperty ;
                                          rdfs:subPropertyOf L1:address ;
                                          L1:level1CDFFName "ALTERNATIVE_LANGUAGE_HEADQUARTERS_ADDRESS"^^xsd:string ,
                                                            "AddressTypeEnum"^^xsd:string ,
                                                            "OtherAddress"^^xsd:string ,
                                                            "OtherAddresses"^^xsd:string ;
                                          L1:level1CDFFType "lei:AddressTypeEnum"^^xsd:string ,
                                                            "lei:OtherAddressType"^^xsd:string ,
                                                            "lei:OtherAddressesType"^^xsd:string ;
                                          rdfs:label "has alternative language headquarters address"@en ;
                                          <http://www.w3.org/2004/02/skos/core#definition> "Address of the headquarters of the entity in the legal jurisdiction, in an alternative language used in the legal jurisdiction."@en .


###  http://lei.info/voc/l1/alternativeLanguageLegalAddress
L1:alternativeLanguageLegalAddress rdf:type owl:ObjectProperty ;
                                   rdfs:subPropertyOf L1:address ;
                                   L1:level1CDFFName "ALTERNATIVE_LANGUAGE_LEGAL_ADDRESS"^^xsd:string ,
                                                     "AddressTypeEnum"^^xsd:string ,
                                                     "OtherAddress"^^xsd:string ,
                                                     "OtherAddresses"^^xsd:string ;
                                   L1:level1CDFFType "lei:AddressTypeEnum"^^xsd:string ,
                                                     "lei:OtherAddressType"^^xsd:string ,
                                                     "lei:OtherAddressesType"^^xsd:string ;
                                   rdfs:label "has alternative language legal address"@en ;
                                   <http://www.w3.org/2004/02/skos/core#definition> "Registered address of the entity in the legal jurisdiction, in an alternative language used in the legal jurisdiction."@en .


###  http://lei.info/voc/l1/associatedEntityWithLEI
L1:associatedEntityWithLEI rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf owl:topObjectProperty ;
                           rdf:type owl:FunctionalProperty ;
                           rdfs:domain L1:LegalEntity ;
                           rdfs:range L1:LegalEntity ;
                           L1:level1CDFFName "AssociatedEntity"^^xsd:string ,
                                             "AssociatedLEI"^^xsd:string ;
                           L1:level1CDFFType "lei:AssociatedEntityType"^^xsd:string ,
                                             "lei:LEIType"^^xsd:string ;
                           rdfs:comment "It is used instead of hasAssociatedEntityWithoutLEI."@en ;
                           rdfs:label "has associated entity with LEI"@en ;
                           <http://www.w3.org/2004/02/skos/core#definition> "The property assigns the entity with LEI associated with the legal entity of this registration."@en .


###  http://lei.info/voc/l1/autoASCIITransliteratedHeadquartersAddress
L1:autoASCIITransliteratedHeadquartersAddress rdf:type owl:ObjectProperty ;
                                              rdfs:subPropertyOf L1:address ;
                                              L1:level1CDFFName "AUTO_ASCII_TRANSLITERATED_HEADQUARTERS_ADDRESS"^^xsd:string ,
                                                                "TransliteratedAddressTypeEnum"^^xsd:string ,
                                                                "TransliteratedOtherAddress"^^xsd:string ,
                                                                "TransliteratedOtherAddresses"^^xsd:string ;
                                              L1:level1CDFFType "lei:TransliteratedAddressTypeEnum"^^xsd:string ,
                                                                "lei:TransliteratedOtherAddressType"^^xsd:string ,
                                                                "lei:TransliteratedOtherAddressesType"^^xsd:string ;
                                              rdfs:label "has auto ASCII transliterated headquarters address"@en ;
                                              <http://www.w3.org/2004/02/skos/core#definition> "Address of the headquarters of the entity, transliterated to ASCII characters, autotransliterated by the managing LOU."@en .


###  http://lei.info/voc/l1/autoASCIITransliteratedLegalAddress
L1:autoASCIITransliteratedLegalAddress rdf:type owl:ObjectProperty ;
                                       rdfs:subPropertyOf L1:address ;
                                       L1:level1CDFFName "AUTO_ASCII_TRANSLITERATED_LEGAL_ADDRESS"^^xsd:string ,
                                                         "TransliteratedAddressTypeEnum"^^xsd:string ,
                                                         "TransliteratedOtherAddress"^^xsd:string ,
                                                         "TransliteratedOtherAddresses"^^xsd:string ;
                                       L1:level1CDFFType "lei:TransliteratedAddressTypeEnum"^^xsd:string ,
                                                         "lei:TransliteratedOtherAddressType"^^xsd:string ,
                                                         "lei:TransliteratedOtherAddressesType"^^xsd:string ;
                                       rdfs:label "has auto ASCII transliterated legal address"@en ;
                                       <http://www.w3.org/2004/02/skos/core#definition> "Registered address of the entity in the legal jurisdiction, transliterated to ASCII characters, autotransliterated by the managing LOU."@en .


###  http://lei.info/voc/l1/entityCategory
L1:entityCategory rdf:type owl:ObjectProperty ,
                           owl:FunctionalProperty ;
                  rdfs:domain L1:LegalEntity ;
                  rdfs:range L1:EntityCategory ;
                  L1:level1CDFFType "EntityCategory"^^xsd:string ;
                  rdfs:label "has legal entity category"@en ;
                  <http://www.w3.org/2004/02/skos/core#definition> "Assigns to a legal entity its category. These categories are based on use cases specified in LEI-ROC policies, found at http://www.leiroc.org/list/leiroc_gls/index.htm (LEI-CDF V2.0)"@en .


###  http://lei.info/voc/l1/entityExpirationReason
L1:entityExpirationReason rdf:type owl:ObjectProperty ,
                                   owl:FunctionalProperty ;
                          rdfs:domain L1:LegalEntity ;
                          rdfs:range L1:EntityExpirationReason ;
                          L1:level1CDFFName "EntityExpirationReason"^^xsd:string ;
                          L1:level1CDFFType "lei:EntityExpirationReasonEnum"^^xsd:string ;
                          rdfs:comment """This element shall be present if entity expiration date is assigned by means of hasEntityExpirationDate datatype property to LE, and omitted otherwise.

See also hasEntityExpirationDate datatype property."""@en ;
                          rdfs:label "has entity expiration reason"@en ;
                          <http://www.w3.org/2004/02/skos/core#definition> "The property assigns to legal entity the reason that it ceased to exist and/or operate."@en .


###  http://lei.info/voc/l1/fundFamily
L1:fundFamily rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf L1:associatedEntityWithLEI ;
              L1:level1CDFFName "FUND_FAMILY"^^xsd:string ;
              L1:level1CDFFType "lei:AssociatedEntityTypeEnum"^^xsd:string ;
              rdfs:label "has fund family"@en ;
              <http://www.w3.org/2004/02/skos/core#definition> "The legal entity is a fund, and the associated entity is the manager of the fund."@en .


###  http://lei.info/voc/l1/headquartersAddress
L1:headquartersAddress rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf L1:address ;
                       rdf:type owl:FunctionalProperty ;
                       L1:level1CDFFName "HeadquartersAddress"^^xsd:string ;
                       L1:level1CDFFType "lei:AddressType"^^xsd:string ;
                       rdfs:label "has headquarters address"@en ;
                       <http://www.w3.org/2004/02/skos/core#definition> "The address of the headquarters of the legal entity."@en .


###  http://lei.info/voc/l1/identifiedBy
L1:identifiedBy rdf:type owl:ObjectProperty ;
                owl:inverseOf L1:identifies ;
                rdf:type owl:FunctionalProperty ,
                         owl:InverseFunctionalProperty ;
                rdfs:domain L1:LegalEntity ;
                rdfs:range L1:LegalEntityIdentifier ;
                L1:level1CDFFName "LEI"^^xsd:string ;
                L1:level1CDFFType "lei:LEIType"^^xsd:string ;
                rdfs:label "is identified by"@en ;
                <http://www.w3.org/2004/02/skos/core#definition> "The property used to assign legal entity identifiers to legal entities."@en .


###  http://lei.info/voc/l1/identifies
L1:identifies rdf:type owl:ObjectProperty ,
                       owl:FunctionalProperty ,
                       owl:InverseFunctionalProperty ;
              rdfs:domain L1:LegalEntityIdentifier ;
              rdfs:range L1:LegalEntity ;
              L1:level1CDFFName "LEI"^^xsd:string ;
              L1:level1CDFFType "lei:LEIType"^^xsd:string ;
              rdfs:label "identifies"@en ;
              <http://www.w3.org/2004/02/skos/core#definition> "The property assigns a (business or global) legal entity identifier to a legal entity."@en .


###  http://lei.info/voc/l1/legalAddress
L1:legalAddress rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf L1:address ;
                rdf:type owl:FunctionalProperty ;
                L1:level1CDFFName "LegalAddress"^^xsd:string ;
                L1:level1CDFFType "lei:AddressType"^^xsd:string ;
                rdfs:label "has legal address"@en ;
                <http://www.w3.org/2004/02/skos/core#definition> "The address of legal formation of the legal entity as recorded in the registration of the entity in its legal jurisdiction."@en .


###  http://lei.info/voc/l1/legalEntityStatus
L1:legalEntityStatus rdf:type owl:ObjectProperty ,
                              owl:FunctionalProperty ;
                     rdfs:domain L1:LegalEntity ;
                     rdfs:range L1:EntityStatus ;
                     L1:level1CDFFType "EntityStatus"^^xsd:string ;
                     rdfs:label "has status"@en ;
                     <http://www.w3.org/2004/02/skos/core#definition> "The property assigns the status (from the class LegalEntityStatus) to legal entity."@en .


###  http://lei.info/voc/l1/managingLOU
L1:managingLOU rdf:type owl:ObjectProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain L1:LegalEntityIdentifier ;
               L1:level1CDFFName "ManagingLOU"^^xsd:string ;
               L1:level1CDFFType "lei:LEIType"^^xsd:string ;
               rdfs:label "has managing LOU"@en ;
               <http://www.w3.org/2004/02/skos/core#definition> "The LOU that is responsible for administering this LEI registration."@en .


###  http://lei.info/voc/l1/otherValidationAuthority
L1:otherValidationAuthority rdf:type owl:ObjectProperty ;
                            rdfs:domain L1:LegalEntity ;
                            rdfs:range L1:ValidationAuthority ;
                            L1:level1CDFFName "OtherValidationAuthorities"^^xsd:string ,
                                              "OtherValidationAuthority"^^xsd:string ;
                            L1:level1CDFFType "lei:OtherValidationAuthoritiesType"^^xsd:string ,
                                              "lei:OtherValidationAuthorityType"^^xsd:string ;
                            rdfs:label "has other validation authority"@en ;
                            <http://www.w3.org/2004/02/skos/core#definition> "An additional registration authority used by the LOU to validate the entity data."@en .


###  http://lei.info/voc/l1/preferredASCIITransliteratedHeadquartersAddress
L1:preferredASCIITransliteratedHeadquartersAddress rdf:type owl:ObjectProperty ;
                                                   rdfs:subPropertyOf L1:address ;
                                                   L1:level1CDFFName "PREFERRED_ASCII_TRANSLITERATED_HEADQUARTERS_ADDRESS"^^xsd:string ,
                                                                     "TransliteratedAddressTypeEnum"^^xsd:string ,
                                                                     "TransliteratedOtherAddress"^^xsd:string ,
                                                                     "TransliteratedOtherAddresses"^^xsd:string ;
                                                   L1:level1CDFFType "lei:TransliteratedAddressTypeEnum"^^xsd:string ,
                                                                     "lei:TransliteratedOtherAddressType"^^xsd:string ,
                                                                     "lei:TransliteratedOtherAddressesType"^^xsd:string ;
                                                   rdfs:label "has preferred ASCII transliterated headquarters address"@en ;
                                                   <http://www.w3.org/2004/02/skos/core#definition> "Registered address of the entity in the legal jurisdiction, transliterated to ASCII characters, provided by the entity for this purpose."@en .


###  http://lei.info/voc/l1/preferredASCIITransliteratedLegalAddress
L1:preferredASCIITransliteratedLegalAddress rdf:type owl:ObjectProperty ;
                                            rdfs:subPropertyOf L1:address ;
                                            L1:level1CDFFName "PREFERRED_ASCII_TRANSLITERATED_LEGAL_ADDRESS"^^xsd:string ,
                                                              "TransliteratedAddressTypeEnum"^^xsd:string ,
                                                              "TransliteratedOtherAddress"^^xsd:string ,
                                                              "TransliteratedOtherAddresses"^^xsd:string ;
                                            L1:level1CDFFType "lei:TransliteratedAddressTypeEnum"^^xsd:string ,
                                                              "lei:TransliteratedOtherAddressType"^^xsd:string ,
                                                              "lei:TransliteratedOtherAddressesType"^^xsd:string ;
                                            rdfs:label "has preferred ASCII transliterated legal address"@en ;
                                            <http://www.w3.org/2004/02/skos/core#definition> "Registered address of the entity in the legal jurisdiction, transliterated to ASCII characters, provided by the entity for this purpose."@en .


###  http://lei.info/voc/l1/primaryValidationAuthority
L1:primaryValidationAuthority rdf:type owl:ObjectProperty ,
                                       owl:FunctionalProperty ;
                              rdfs:domain L1:LegalEntity ;
                              rdfs:range L1:ValidationAuthority ;
                              L1:level1CDFFType "ValidationAuthorityType"^^xsd:string ;
                              rdfs:label "has primary validation authority"@en ;
                              <http://www.w3.org/2004/02/skos/core#definition> "The (primary) registration authority used by the LOU to validate GLEI data."@en .


###  http://lei.info/voc/l1/registrationAuthority
L1:registrationAuthority rdf:type owl:ObjectProperty ;
                         rdfs:domain L1:LegalEntity ;
                         L1:level1CDFFType "RegistrationAuthorityType"^^xsd:string ;
                         rdfs:label "has registration authority"@en ;
                         <http://www.w3.org/2004/02/skos/core#definition> "A registration authority"@en .


###  http://lei.info/voc/l1/registrationStatus
L1:registrationStatus rdf:type owl:ObjectProperty ,
                               owl:FunctionalProperty ;
                      rdfs:domain L1:LegalEntityIdentifier ;
                      rdfs:range L1:RegistrationStatus ;
                      L1:level1CDFFName "RegistrationStatus"^^xsd:string ;
                      L1:level1CDFFType "lei:RegistrationStatusEnum"^^xsd:string ;
                      rdfs:comment """This is not to be confused with the status of the legal entity itself; see hasEntityStatus.

See also RegistrationStatus class."""@en ;
                      rdfs:label "has registration status"@en ;
                      <http://www.w3.org/2004/02/skos/core#definition> "The status of the legal entity's LEI registration with the Managing LOU."@en .


###  http://lei.info/voc/l1/successorEntityWithLEI
L1:successorEntityWithLEI rdf:type owl:ObjectProperty ,
                                   owl:FunctionalProperty ;
                          rdfs:domain L1:LegalEntity ;
                          rdfs:range L1:LegalEntity ;
                          L1:level1CDFFName "SuccessorEntity"^^xsd:string ,
                                            "SuccessorLEI"^^xsd:string ;
                          L1:level1CDFFType "lei:LEIType"^^xsd:string ,
                                            "lei:SuccessorEntityType"^^xsd:string ;
                          rdfs:label "has successor entity with LEI"@en ;
                          <http://www.w3.org/2004/02/skos/core#definition> """The surviving/new legal entity which continues/replaces this registration. 

The successor entity should have LEI."""@en .


###  http://lei.info/voc/l1/validationSource
L1:validationSource rdf:type owl:ObjectProperty ,
                             owl:FunctionalProperty ;
                    rdfs:domain L1:LegalEntity ;
                    rdfs:range L1:ValidationSource ;
                    L1:level1CDFFType "ValidationSources"^^xsd:string ;
                    rdfs:comment "It is omitted if the validation status is not known or not revealed."@en ;
                    rdfs:label "has validation source"@en ;
                    <http://www.w3.org/2004/02/skos/core#definition> "The property assigns the current validation status (from ValidationSource class) to this LEI record."@en .


###  http://lei.info/voc/l2/childLegalEntity
L2:childLegalEntity rdf:type owl:ObjectProperty ;
                    rdfs:domain L2:Relationship ;
                    rdfs:range L2:LegalEntity ;
                    L2:level2CDFFName "NodeID"^^xsd:string ,
                                      "StartNode"^^xsd:string ;
                    L2:level2CDFFType "rr:LEIType"^^xsd:string ,
                                      "rr:NodeType"^^xsd:string ;
                    rdfs:label "has child legal entity"@en ;
                    <http://www.w3.org/2004/02/skos/core#definition> "The entity at the \"start\" of a directional relationship."@en .


###  http://lei.info/voc/l2/managingLOU
L2:managingLOU rdf:type owl:ObjectProperty ;
               rdfs:domain L2:Relationship ;
               rdfs:range L2:LocalOperatingUnit ;
               L2:level2CDFFName "ManagingLOU"^^xsd:string ;
               L2:level2CDFFType "rr:LEIType"^^xsd:string ;
               rdfs:label "has managing LOU"@en ;
               <http://www.w3.org/2004/02/skos/core#definition> "The LOU that is responsible for administering this relationship record."@en .


###  http://lei.info/voc/l2/measurementMethod
L2:measurementMethod rdf:type owl:ObjectProperty ;
                     rdfs:domain L2:RelationshipQuantifier ;
                     rdfs:range L2:MeasurementMethod ;
                     L2:level2CDFFName "MeasurementMethod"^^xsd:string ;
                     L2:level2CDFFType "rr:RelationshipQuantifierType"^^xsd:string ;
                     rdfs:label "has measurement method"@en ;
                     <http://www.w3.org/2004/02/skos/core#definition> "This property specifies the method of measurement (or set of rules) used to quantitatively categorize the relationship."@en .


###  http://lei.info/voc/l2/parentLegalEntity
L2:parentLegalEntity rdf:type owl:ObjectProperty ;
                     rdfs:domain L2:Relationship ;
                     rdfs:range L2:LegalEntity ;
                     L2:level2CDFFName "EndNode"^^xsd:string ,
                                       "NodeID"^^xsd:string ;
                     L2:level2CDFFType "rr:LEIType"^^xsd:string ,
                                       "rr:NodeType"^^xsd:string ;
                     rdfs:label "has parent legal entity"@en ;
                     <http://www.w3.org/2004/02/skos/core#definition> "The entity at the \"end\" of a directional relationship."@en .


###  http://lei.info/voc/l2/qualifierCategory
L2:qualifierCategory rdf:type owl:ObjectProperty ;
                     rdfs:domain L2:RelationshipQualifier ;
                     rdfs:range L2:RelationshipQualifierCategory ;
                     L2:level2CDFFName "QualifierCategory"^^xsd:string ;
                     L2:level2CDFFType "rr:QualifierCategoryTypeEnum"^^xsd:string ;
                     rdfs:label "has qualifier category"@en ;
                     <http://www.w3.org/2004/02/skos/core#definition> "This property specifies the additional qualitative attributes that help to categorize the relationship."@en .


###  http://lei.info/voc/l2/qualifierDimension
L2:qualifierDimension rdf:type owl:ObjectProperty ;
                      rdfs:domain L2:RelationshipQualifier ;
                      rdfs:range L2:RelationshipQualifierDimension ;
                      L2:level2CDFFName "QualifierDimension"^^xsd:string ;
                      L2:level2CDFFType "rr:QualifierDimensionEnum"^^xsd:string ;
                      rdfs:label "has qualifier dimension"@en ;
                      <http://www.w3.org/2004/02/skos/core#definition> "This property designates the optional list of additional qualitative attributes that help to categorize the relationship."@en .


###  http://lei.info/voc/l2/quantifierUnit
L2:quantifierUnit rdf:type owl:ObjectProperty ;
                  rdfs:domain L2:RelationshipQuantifier ;
                  rdfs:range L2:RelationshipQuantifierUnit ;
                  L2:level2CDFFName "QuantifierUnits"^^xsd:string ;
                  L2:level2CDFFType "rr:QuantifierUnitsTypeEnum"^^xsd:string ;
                  rdfs:comment "enumeration PERCENTAGE"@en ;
                  rdfs:label "has quantifier unit"@en ;
                  <http://www.w3.org/2004/02/skos/core#definition> "This property specifies the units, where applicable, of a measurement made on a relationship."@en .


###  http://lei.info/voc/l2/registrationStatus
L2:registrationStatus rdf:type owl:ObjectProperty ;
                      rdfs:domain L2:Relationship ;
                      rdfs:range L2:RegistrationStatus ;
                      L2:level2CDFFName "RegistrationStatus"^^xsd:string ;
                      L2:level2CDFFType "rr:RegistrationStatusEnum"^^xsd:string ;
                      rdfs:comment """This is not to be confused with the status of the relationship itself; see hasRelationshipStatus.

See also RegistrationStatus class."""@en ;
                      rdfs:label "has registration status"@en ;
                      <http://www.w3.org/2004/02/skos/core#definition> "The status of the legal entity's relationship record registration with the managing LOU."@en .


###  http://lei.info/voc/l2/relationshipPeriod
L2:relationshipPeriod rdf:type owl:ObjectProperty ;
                      rdfs:domain L2:Relationship ;
                      rdfs:range [ rdf:type owl:Class ;
                                   owl:unionOf ( L2:AccountingPeriod
                                                 L2:DocumentFilingPeriod
                                                 L2:RelationshipPeriod
                                               )
                                 ] ;
                      L2:level2CDFFName "RelationshipPeriod"^^xsd:string ;
                      L2:level2CDFFType "rr:RelationshipPeriodType"^^xsd:string ;
                      rdfs:label "has relationship period"@en ;
                      <http://www.w3.org/2004/02/skos/core#definition> "This property assigns one set of start and end dates for a particular type of period, for example, the duration of the relationship itself, the filing or validity period of any documents demonstrating the relationship, or the accounting period they refer to."@en .


###  http://lei.info/voc/l2/relationshipQualifier
L2:relationshipQualifier rdf:type owl:ObjectProperty ;
                         rdfs:domain L2:Relationship ;
                         rdfs:range L2:RelationshipQualifier ;
                         L2:level2CDFFName "RelationshipQualifier"^^xsd:string ;
                         L2:level2CDFFType "rr:RelationshipQualifierType"^^xsd:string ;
                         rdfs:label "has relationship qualifier"@en ;
                         <http://www.w3.org/2004/02/skos/core#definition> "This property specifies relationship qualifier information."@en .


###  http://lei.info/voc/l2/relationshipQuantifier
L2:relationshipQuantifier rdf:type owl:ObjectProperty ;
                          rdfs:domain L2:Relationship ;
                          rdfs:range L2:RelationshipQuantifier ;
                          L2:level2CDFFName "RelationshipQuantifier"^^xsd:string ;
                          L2:level2CDFFType "rr:RelationshipQuantifierType"^^xsd:string ;
                          rdfs:label "has relationship quantifier"@en ;
                          <http://www.w3.org/2004/02/skos/core#definition> "This property specifies any additional quantitative attributes that help to categorize the relationship."@en .


###  http://lei.info/voc/l2/relationshipStatus
L2:relationshipStatus rdf:type owl:ObjectProperty ;
                      rdfs:domain L2:Relationship ;
                      rdfs:range L2:RelationshipStatus ;
                      L2:level2CDFFName "RelationshipStatus"^^xsd:string ;
                      L2:level2CDFFType "rr:RelationshipStatusEnum"^^xsd:string ;
                      rdfs:label "has relationship status"@en ;
                      <http://www.w3.org/2004/02/skos/core#definition> "This property assigns the status of the legal entities' relationship itself: ACTIVE or INACTIVE."@en .


###  http://lei.info/voc/l2/relationshipType
L2:relationshipType rdf:type owl:ObjectProperty ;
                    rdfs:domain L2:Relationship ;
                    rdfs:range L2:RelationshipType ;
                    L2:level2CDFFName "RelationshipType"^^xsd:string ;
